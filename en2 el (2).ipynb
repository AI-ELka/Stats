   "source": [
    "# l'intervalle de confiance\n",
    "c = stats.t.ppf(1 - alpha/2, n + p - 2)*((np.sqrt(sigma_2)*(n+p))/np.sqrt((n+p-2)*n*p))\n",
    "intervalle = [m0-m1 - c , m0-m1 + c ]\n",
    "print(\"L'intervalle de confiance :\", intervalle)"
   ]
